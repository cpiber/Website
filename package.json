{
  "name": "website",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build:js && npm run build:blocks && gulp sass:build && gulp sassmodules:build",
    "build:js": "wp-scripts build src/frontend/index.js --output-path=build/frontend",
    "build:blocks": "wp-scripts build src/blocks/index.js --output-path=build/blocks",
    "build:css": "postcss \"src/css/**/!(*.module).scss\" --base src/css --dir build/css --ext css",
    "build:cssmodules": "CSS_ENV=modules postcss \"src/css/**/[!_]*.module.scss\" --base src/css --dir build/css --ext css",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "start": "concurrently \"npm:start:js\" \"npm:start:blocks\" \"npm:start:css\" \"npm:start:cssmodules\"",
    "start:js": "wp-scripts start src/frontend/index.js --output-path=build/frontend",
    "start:blocks": "wp-scripts start src/blocks/index.js --output-path=build/blocks",
    "start:css": "gulp sass:watch",
    "start:cssmodules": "gulp sassmodules:watch",
    "packages-update": "wp-scripts packages-update",
    "clean": "rimraf build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/runtime": "^7.12.13",
    "@types/wordpress__block-editor": "^2.2.9",
    "@types/wordpress__blocks": "^6.4.12",
    "@wordpress/block-editor": "^5.2.7",
    "@wordpress/blocks": "^7.0.3",
    "@wordpress/components": "^12.0.6",
    "@wordpress/compose": "^3.24.3",
    "@wordpress/data": "^4.26.6",
    "@wordpress/env": "^3.0.2",
    "@wordpress/i18n": "^3.18.0",
    "@wordpress/scripts": "^13.0.3",
    "concurrently": "^5.3.0",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-modules": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^3.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.0.0-0",
    "react-native": ">=0.14.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7"
  }
}